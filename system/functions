function npmversion() {
  npm v $1 | grep -A 50 dist-tags | grep -vE "published|dist-tags"
}

function rebase() {
  git co $1 && git pull upstream $1 && git push && git co - && git rebase $1
}

function key() {
  eval "$(ssh-agent -s)" && ssh-add ~/.ssh/$1
}

function prfix() {
  git add . && git commit -m 'fix: pr comments'
}